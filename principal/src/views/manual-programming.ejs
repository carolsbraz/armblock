            <div class="configuration">
                <div class="configuration-blocks">
                    <div class="green title-block block">Configurações</div>
                    <div class="brown serial-block block">Selecione a porta serial:
                        <select name="serial-port" id="serial-port">
                            <% for (porta of portas){%>
                                <option value="<%= porta %>"><%= porta %></option>
                              <% } %>
                </select>
                    </div>
                    <div class="dark-blue pin-block block">Pino do motor 1: <input type="number" placeholder="pino" name="pino1"></div>
