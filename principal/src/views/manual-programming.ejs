<body>

    <header>
        <div class="brand">
            <img src="/img/robotic-arm.svg" alt="logo" width="80px">
            <h2 class="title">Educational ArmBlock</h2>
        </div>
        <div class="menu">
            <div class="link-menu"><a href="/" class="link">Voltar ao menu</a></div>
            <div class="buttons">
                <div class="change-button">
                    <a href="#" class="rotine">Rotina</a>
                    <a href="#" class="operation">Operação</a>
                </div>

            </div>
        </div>
    </header>

    <form action="/enviar-dados" method="get">
